{
  "name": "MouseRun",
  "description": "An example chrome extension that injects buttons using jQuery",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://*/*",
        "http://*/*",
        "*://*/*"
      ],
      "js": [
        "js/jquery.js",
        "js/script.js"
      ]
    }
  ],
  "manifest_version": 2
}